    130          /*===========================================================================
    131          * 函数 : DelayMs() => 延时函数(ms级)                                        *
    132          * 输入 ：x, 需要延时多少(0-65535)                                             *
    133          ============================================================================*/
    134          void DelayMs(u16 x)
    135          {
    136              volatile u16 timer_ms = x;
    137              
    138              Cnt1ms = 0;
    139              TIM3_Set(1);
    140              while(Cnt1ms <= timer_ms);
    141              TIM3_Set(0);
    142          }
